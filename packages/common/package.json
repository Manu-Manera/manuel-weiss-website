    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.156",
    "aws-sdk": "^2.1500.0",
    "crypto": "^1.0.1",
    "openai": "^4.20.0",
    "uuid": "^9.0.0",
    "winston": "^3.18.3",
